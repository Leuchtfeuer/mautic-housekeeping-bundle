{
    "name": "leuchtfeuer/leuchtfeuer-housekeeping-bundle",
    "type": "mautic-plugin",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Leuchtfeuer Digital Marketing GmbH",
            "email": "mautic@Leuchtfeuer.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0.2",
        "ext-pdo": "*",
        "mautic/core-lib": "^5.0"
    },
    "extra": {
        "install-directory-name": "LeuchtfeuerHousekeepingBundle"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "MauticPlugin\\MauticHousekeepingBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MauticPlugin\\MauticHousekeepingBundle\\Tests\\": "Tests"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/dennisameling/FOSOAuthServerBundle.git"
        },
        {
            "type": "git",
            "url": "https://github.com/mautic/BazingaOAuthServerBundle.git"
        }
    ],
    "scripts": {
        "cs-fix": "./vendor/bin/php-cs-fixer fix --config .php-cs-fixer.php --using-cache no --show-progress dots -v"
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
